    {
        "id": "b935e850.554d58",
        "type": "function",
        "z": "9cba5f25.24967",
        "name": "CMD ?",
        "func": "if(msg.payload.cmd === 'START'){\n    context.global.dataToSend = 'MP. ';\n    msg.payload = msg.payload.cmd;\n    node.send(msg);\n} \nif(msg.payload.cmd === 'STOP'){\n    context.global.dataToSend = 'MP. ';\n    msg.payload = msg.payload.cmd;\n    node.send(msg);\n} \nif(msg.payload.cmd === 'SEN.'){\n    context.global.dataToSend = 'SEN.' + msg.payload.data;\n    node.send(msg);\n    setTimeout(function(){\n        context.global.dataToSend = 'MP. ';\n    },global.get('keepCMD'));\n} \nif(msg.payload.cmd === 'DATA?'){\n    msg.payload = msg.payload.cmd;\n    node.send(msg);\n}",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
