    {
        "id": "2b46612e.17705e",
        "type": "function",
        "z": "9cba5f25.24967",
        "name": "CleanSerialAnswer",
        "func": "/*\nif(msg.payload.substr(0,4)!=='MAG.'){\n    msg.payload = parseFloat(msg.payload);\n    if (!isNaN(parseFloat(msg.payload))) return msg;\n}\n*/\n\nif(msg.payload.substr(0,3)!=='MP.'){\n    const exp_reg = /(?=E)/gi ;\n    var resultat = msg.payload.match(exp_reg);\n    if(resultat){\n        split = msg.payload.split(',');\n        if(split.length==2){\n            var msg2 = {} ;\n            msg.payload = parseFloat(split[0]);\n            global.set('M', msg.payload);\n            msg.topic = 'M';\n            msg2.payload = parseFloat(split[1]);\n            global.set('P', msg.payload);\n            msg2.topic = 'P';\n            if (!isNaN(parseFloat(msg.payload))&&!isNaN(parseFloat(msg2.payload))) return [msg,msg2];\n        }\n    }\n}\n\n\n",
